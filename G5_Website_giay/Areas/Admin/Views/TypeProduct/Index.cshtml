@model IEnumerable<G5_Website_giay.Models.LoaiSanPham>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            TypeID
        </th>
        <th>
            Type
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.lspID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lspName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.lspID  }) |
            @Html.ActionLink("Details", "Details", new { id = item.lspID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.lspID })
        </td>
    </tr>
}

</table>
